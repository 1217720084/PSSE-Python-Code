import psspy

ierr, busnumbers = psspy.abusint(sid=2,string="NUMBER")
ierr, busnames = psspy.abuschar(sid=2,string="NAME")
ierr, busvoltages = psspy.abusreal(sid=2,string="PU")
ierr, bus_shunts = psspy.abuscplx(sid=2, string = "SHUNTACT")


